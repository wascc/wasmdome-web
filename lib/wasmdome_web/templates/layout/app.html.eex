                    <button class="c-class-toggler c-header-nav-btn" type="button" id="header-tooltip"
                        data-target="body" data-class="c-dark-theme" data-toggle="c-tooltip" data-placement="bottom"
                        title="Toggle Light/Dark Mode">
                        <svg class="c-icon c-d-dark-none">
                            <use xlink:href="<%= Routes.static_path(@conn, "/coreui/sprites/free.svg#cil-moon") %>"></use>
                        </svg>
                        <svg class="c-icon c-d-default-none">
                            <use xlink:href="<%= Routes.static_path(@conn, "/coreui/sprites/free.svg#cil-sun") %>"></use>
                        </svg>
                    </button>
                </li>
            </ul>
            <ul class="c-header-nav">
                                                
                <%# ACCOUNT %>
                <%= if @current_user do %>
                  <!--<li class="active"><a href="/logout">Logout</a> (<%= @current_user.name %>)</li>-->
                  <li class="c-header-nav-item dropdown"><a class="c-header-nav-link" data-toggle="dropdown" href="#"
                        role="button" aria-haspopup="true" aria-expanded="false">
                        <div class="c-avatar"><img class="c-avatar-img" src="<%= @current_user.avatar %>"
                                alt="user@email.com"></div>
                    </a>
                    <div class="dropdown-menu dropdown-menu-right pt-0">
